	<property>gear/gear[0]/position-norm</property>
	<object-name>UC-Nose-Retract</object-name>
	<axis>
		<y>1</y>
	</axis>
	<center>
		<x-m>3.645</x-m>
		<z-m>-0.46</z-m>
	</center>
	<interpolation>
		<entry> <ind>0</ind> <dep>0</dep> </entry>
