<?xml version="1.0"?>

<PropertyList>
	<path>Trajectory-Marker.ac</path>
	<animation> 
		<type>range</type>
		<min-m>5</min-m>
		<max-m>10000</max-m>
	</animation>
	<animation>
		<type>alpha-test</type>
		<alpha-factor>0.01</alpha-factor>
	</animation>
	<animation>
		<type>billboard</type>
		<spherical type="bool">true</spherical>
	</animation>
	<animation>
		<type>dist-scale</type>
		<interpolation>
			<entry> <ind>0</ind>     <dep>0.5</dep> </entry>
			<entry> <ind>500</ind>   <dep>0.8</dep> </entry>
			<entry> <ind>10000</ind> <dep>10</dep>  </entry>
		</interpolation>
	</animation>
</PropertyList>
